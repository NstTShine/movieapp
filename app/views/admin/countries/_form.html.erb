<%= form_for [:admin, @country] do |f| %>
  <%= render "shared/error_messages",object: @country %>
  <%= f.label :name %>
  <%= f.text_field :name, class: "form-control", required: "true" %>

  <%= f.submit (yield :button_text), class: "btn btn-primary mg-top" %>
  <%= link_to (t ".cancel"), admin_countries_path, class: "btn btn-default mg-top" %>
<% end %>
